<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <FrameLayout
        android:id="@+id/fr_"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp" />

    <com.luseen.spacenavigation.SpaceNavigationView
        android:id="@+id/spacenavigation_user"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:space_background_color="@color/colorAccent"
        app:inactive_item_color="@color/text"
        app:active_item_color="@color/dot_light_screen2"
        app:centre_button_color="@color/text"
        app:centre_button_icon="@drawable/info"
        app:active_centre_button_icon_color="@color/colorAccent"
        app:inactive_centre_button_icon_color="@color/colorAccent"
        android:layout_gravity="bottom"
        tools:ignore="MissingConstraints" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>